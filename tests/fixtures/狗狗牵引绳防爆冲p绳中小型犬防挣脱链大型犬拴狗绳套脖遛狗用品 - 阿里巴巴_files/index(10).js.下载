      </${t}>
    `}};function bound01(e,t){isOnePointZero(e)&&(e="100%");const r=isPercentage(e);return e=360===t?e:Math.min(t,Math.max(0,parseFloat(e))),r&&(e=parseInt(String(e*t),10)/100),Math.abs(e-t)<1e-6?1:e=360===t?(e<0?e%t+t:e%t)/parseFloat(String(t)):e%t/parseFloat(String(t))}function clamp01(e){return Math.min(1,Math.max(0,e))}function isOnePointZero(e){return"string"==typeof e&&-1!==e.indexOf(".")&&1===parseFloat(e)}function isPercentage(e){return"string"==typeof e&&-1!==e.indexOf("%")}function boundAlpha(e){return e=parseFloat(e),(isNaN(e)||e<0||e>1)&&(e=1),e}function convertToPercentage(e){return Number(e)<=1?100*Number(e)+"%":e}function pad2(e){return 1===e.length?"0"+e:String(e)}function rgbToRgb(e,t,r){return{r:255*bound01(e,255),g:255*bound01(t,255),b:255*bound01(r,255)}}function rgbToHsl(e,t,r){e=bound01(e,255),t=bound01(t,255),r=bound01(r,255);const o=Math.max(e,t,r),s=Math.min(e,t,r);let i=0,n=0;const a=(o+s)/2;if(o===s)n=0,i=0;else{const l=o-s;switch(n=a>.5?l/(2-o-s):l/(o+s),o){case e:i=(t-r)/l+(t<r?6:0);break;case t:i=(r-e)/l+2;break;case r:i=(e-t)/l+4}i/=6}return{h:i,s:n,l:a}}function hue2rgb(e,t,r){return r<0&&(r+=1),r>1&&(r-=1),r<1/6?e+6*r*(t-e):r<.5?t:r<2/3?e+(t-e)*(2/3-r)*6:e}function hslToRgb(e,t,r){let o,s,i;if(e=bound01(e,360),t=bound01(t,100),r=bound01(r,100),0===t)s=r,i=r,o=r;else{const n=r<.5?r*(1+t):r+t-r*t,a=2*r-n;o=hue2rgb(a,n,e+1/3),s=hue2rgb(a,n,e),i=hue2rgb(a,n,e-1/3)}return{r:255*o,g:255*s,b:255*i}}function rgbToHsv(e,t,r){e=bound01(e,255),t=bound01(t,255),r=bound01(r,255);const o=Math.max(e,t,r),s=Math.min(e,t,r);let i=0;const n=o,a=o-s,l=0===o?0:a/o;if(o===s)i=0;else{switch(o){case e:i=(t-r)/a+(t<r?6:0);break;case t:i=(r-e)/a+2;break;case r:i=(e-t)/a+4}i/=6}return{h:i,s:l,v:n}}function hsvToRgb(e,t,r){e=6*bound01(e,360),t=bound01(t,100),r=bound01(r,100);const o=Math.floor(e),s=e-o,i=r*(1-t),n=r*(1-s*t),a=r*(1-(1-s)*t),l=o%6;return{r:255*[r,n,i,i,a,r][l],g:255*[a,r,r,n,i,i][l],b:255*[i,i,a,r,r,n][l]}}function rgbToHex(e,t,r,o){const s=[pad